from django.db import models


{% for model in models %}
class {{model.name}}(models.Model):  
{% for field in model.fields %}
    {{ field.name }} = models.{{ field.type }}(
        {% for k, v in field.extras.items() %}
            {% if loop.last %}
                {{ k }}={% if v is string %}{{ v|tojson }}{% else %}{{ v }}{% endif %}
            {% else %}
                {{ k }}={% if v is string %}{{ v|tojson }},{% else %}{{ v }},{% endif %}
            {% endif %}
        {% endfor %}
        )
{% endfor %}
{% endfor %}
